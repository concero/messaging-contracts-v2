{
    "name": "@concero/contracts-v2",
    "version": "1.0.0",
    "license": "UNLICENSED",
    "bugs": "security@concero.io",
    "homepage": "https://concero.io",
    "description": "Concero V2 Contracts. If you discover any security vulnerabilities, report them responsibly to security@concero.io. Thank you!",
    "scripts": {
        "chain": "hardhat node --network hardhat --no-deploy",
        "fork": "ETHEREUM_FORKING_ENABLED=true hardhat node --network hardhat --no-deploy",
        "compile": "hardhat compile",
        "test": "REPORT_GAS=true hardhat test --network localhost",
        "format": "npm run format:ts && npm run format:sol",
        "lint": "npm run lint:ts && npm run lint:sol",
        "lint:ts": "eslint --config .eslint.config.mjs",
        "lint:sol": "solhint --config .solhint.json ./contracts/*.sol ./contracts/**/*.sol",
        "format:ts": "prettier --config .prettierrc.json --write *.ts **/*.ts **/**/*.ts **/**/**/*.ts",
        "format:sol": "prettier --config .prettierrc.json --write ./contracts/**/*.sol ./test/**/*.sol",
        "clean:hardhat": "rm -rf ./artifacts ./cache ./coverage ./typechain-types ./deployments",
        "clean:forge": "forge clean",
        "clean": "npm run clean:hardhat && npm run clean:forge",
        "build:messageReport": "bun build ./clf/src/messageReport/test.ts --outfile ./clf/dist/messageReport.min.js --target node",
        "build:operatorRegistration": "bun build ./clf/src/operatorRegistration/index.ts --outfile ./ clf/dist/operatorRegistration.min.js --target node",
        "size-contracts": "hardhat size-contracts",
        "check-updates": "npx npm-check-updates",
        "prepare": "husky"
    },
    "devDependencies": {
        "@nomicfoundation/hardhat-chai-matchers": "^2.0.8",
        "@nomicfoundation/hardhat-ethers": "^3.0.8",
        "@nomicfoundation/hardhat-network-helpers": "^1.0.12",
        "@nomicfoundation/hardhat-verify": "^2.0.12",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@typechain/hardhat": "^9.1.0",
        "@types/prettier": "^3.0.0",
        "@types/qrcode": "^1.5.5",
        "@typescript-eslint/eslint-plugin": "^8.22.0",
        "@typescript-eslint/parser": "^8.22.0",
        "chai": "4.3.7",
        "eslint": "^9.19.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "ethers-v5": "npm:ethers@5.7.2",
        "hardhat": "^2.22.18",
        "hardhat-contract-sizer": "^2.10.0",
        "hardhat-deploy": "^0.14.0",
        "hardhat-deploy-ethers": "^0.4.2",
        "hardhat-docgen": "^1.3.0",
        "hardhat-gas-reporter": "^2.2.2",
        "husky": "^9.1.7",
        "ora": "^8.1.1",
        "prettier": "^3.4.2",
        "prettier-plugin-solidity": "^1.4.2",
        "solhint": "^5.0.5",
        "solhint-community": "^4.0.1",
        "solhint-plugin-prettier": "^0.1.0",
        "solidity-coverage": "^0.8.14",
        "ts-node": "^10.9.2",
        "typechain": "^8.3.2",
        "typescript": "^5.7.3",
        "@chainlink/env-enc": "1.0.5"
    },
    "dependencies": {
        "@chainlink/contracts": "^1.3.0",
        "@chainlink/contracts-ccip": "^1.5.0",
        "@chainlink/env-enc": "^1.0.5",
        "@chainlink/functions-toolkit": "^0.3.2",
        "@chainlink/hardhat-chainlink": "^0.0.4",
        "@chainlink/local": "^0.2.3",
        "@chainlink/solhint-plugin-chainlink-solidity": "^1.0.1",
        "@concero/v2-operators": "github:concero/v2-operators",
        "@nomicfoundation/hardhat-ignition-viem": "^0.15.9",
        "@nomicfoundation/hardhat-viem": "^2.0.6",
        "@openzeppelin/contracts": "5.2.0",
        "@tenderly/hardhat-tenderly": "^2.5.2",
        "@typechain/ethers-v6": "^0.5.1",
        "@uniswap/swap-router-contracts": "https://github.com/Uniswap/swap-router-contracts",
        "@uniswap/v2-periphery": "https://github.com/Uniswap/v2-periphery",
        "@uniswap/v3-periphery": "https://github.com/sushiswap/v3-periphery",
        "asciichart": "^1.5.25",
        "dotenv": "^16.4.7",
        "ds-test": "https://github.com/dapphub/ds-test.git",
        "envfile": "^7.1.0",
        "ethers": "6.13.5",
        "forge-std": "https://github.com/foundry-rs/forge-std.git",
        "readline": "^1.3.0",
        "regenerator-runtime": "^0.14.1",
        "solady": "^0.1.1",
        "solhint-plugin-chainlink-solidity": "npm:@chainlink/solhint-plugin-chainlink-solidity",
        "solidity-bytes-utils": "0.8.2",
        "sushiswap-v3-periphery": "https://github.com/sushiswap/v3-periphery",
        "viem": "^2.22.17"
    }
}
